<!DOCTYPE html>
<html>
<head>
	<title>Profile Info</title>
	<link rel="stylesheet" type="text/css" href="/abc/css/style.css">
</head>
<body>
	<form method="post">
		<fieldset>
			<legend>Profile Information</legend>

			<a href="/Mhome">Back</a> |
	        <a href="/logout">logout</a>
	        <br>
	        <br>
			<table>
				<tr>
					<td>Username :</td>
				    <td><%= users.UserName %></td>
				</tr>

				<tr>
					<td>Name :</td>
				    <td><%= users.Name %></td>
				</tr>

				<tr>
				    <td>Password :</td>
				    <td><%= users.Password %></td>
			    </tr>

			    <tr>
				    <td>Email :</td>
				    <td><%= users.Email %></td>
			    </tr>

			     <tr>
				    <td>Contact No :</td>
				    <td><%= users.ContactNo %></td>
			    </tr>

			     <tr>
				    <td>Gender :</td>
				    <td><%= users.Gender %></td>
			    </tr>

			    <tr>
				    <td>Type :</td>
				    <td><%= users.UserType %></td>
			    </tr>

			     <tr>
				    <td>Address :</td>
				    <td><%= users.Address %></td>
			    </tr>


			    <tr>
					<td>Want to update Profile !
					<a href="/user/update">Update</a> </td>
				</tr>
     
				
		</fieldset>
	</form>
</body>
</html>